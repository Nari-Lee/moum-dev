<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="moum.project.dao.MaincategoryDao">

  <resultMap id="MaincategoryMap" type="moum.project.vo.Maincategory">
    <id column="maincategory_id" property="no"/>
    <result column="category_name" property="name"/>
  </resultMap>

  <insert id="insert" parameterType="moum.project.vo.Maincategory">

  </insert>

  <select id="list" resultMap="MaincategoryMap">
    select
      maincategory_id,
      category_name
    from
      maincategory
    where
      maincategory_id != 999
  </select>

  <select id="findBy">

  </select>

  <update id="update">

  </update>

  <delete id="delete">

  </delete>
</mapper>
