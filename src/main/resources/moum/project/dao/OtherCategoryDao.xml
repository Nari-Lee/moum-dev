<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="moum.project.dao.OtherCategoryDao">

  <resultMap id="OtherCategoryMap" type="moum.project.vo.OtherCategory">
    <result column="category_name" property="name"/>
    <result column="date" property="date"/>
  </resultMap>

  <insert id="insert" parameterType="moum.project.vo.OtherCategory">
    insert into other_category(category_name)
    values(#{name})
  </insert>

  <select id="list" resultMap="OtherCategoryMap">

  </select>

  <select id="findBy">

  </select>

  <update id="update">

  </update>

  <delete id="delete">

  </delete>
</mapper>
